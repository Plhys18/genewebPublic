    "version": "0.2.0",
    "configurations": [
        {
            "name": "pipeline Physco",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Physcomitrella_patens"],
            "type": "dart"
        },
        {
            "name": "pipeline Arabidopsis",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Arabidopsis_thaliana"],
            "type": "dart"
        },
        {
            "name": "pipeline Arabidopsis TSS",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Arabidopsis_thaliana", "--with-tss"],
            "type": "dart"
        },
        {
            "name": "pipeline Small RNA",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Arabidopsis_small_rna"],
            "type": "dart"
        },
        {
            "name": "pipeline Mitochondrion",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Arabidopsis_thaliana_mitochondrion"],
            "type": "dart"
        },
        {
            "name": "pipeline Chloroplast",
            "cwd": "pipeline",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline/bin/pipeline.dart",
            "args": ["Arabidopsis_thaliana_chloroplast"],
            "type": "dart"
        },
        {
            "name": "ui",
            "cwd": "ui",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "ui (profile mode)",
            "cwd": "ui",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "ui (release mode)",
            "cwd": "ui",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}